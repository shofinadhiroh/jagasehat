<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frame_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment3Salira">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#F4D2D0"
        tools:context=".Fragment2Mengapa">

        <ImageView
            android:id="@+id/imageJudul"
            android:layout_width="397dp"
            android:layout_height="96dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:src="@drawable/salira" />

        <Button
            android:id="@+id/buttonsalira1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="98dp"
            android:layout_marginStart="102dp"
            android:background="#D0566D"
            android:padding="10dp"
            android:text="Cegah sejak dini"
            android:textColor="#F7D4D2" />

        <Button
            android:id="@+id/buttonsalira2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:layout_marginBottom="32dp"
            android:layout_marginStart="87dp"
            android:background="#D0566D"
            android:padding="10dp"
            android:text="Konsultasi ke dokter"
            android:textColor="#F7D4D2" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="13dp"
            android:src="@drawable/sadari" />

    </RelativeLayout>
</FrameLayout>